        <script type="text/javascript">
            $(function () {
                // Request to specific server
                $('#start_testing').click(function() {
                    $.ajax({
                        url: "http://127.0.0.1:62590/requestManage",
                        type: "get",
                        dataType: "json",
                        cache: false,
                        timeout: 30000,
                        contentType: "application/json",
                        success: function (responseData) {

                        }, error: function (xhr, textStatus, errorThrown) {

                        }
                    });
                });

                // 기본적으로 1depth의 드롭다운을 로드함
                $("#testprofile").load("TestcaseList.html #testprofile1-1", function() {
                    // 로딩이 완료되면 드롭다운을 감싸는 태그 요소를 화면에 표시함
                    $(this).show();
                });

                // 기본적으로 1depth의 드롭다운을 로드함
                $("#testcaselist").load("TestcaseList.html #testcaselist1-1", function() {
                    // 로딩이 완료되면 드롭다운을 감싸는 태그 요소를 화면에 표시함
                    $(this).show();
                });

                $(document).on("change", "#testprofile > select", function() {
                    // 1dpeth가 변경되므로 2,3depth는 초기화.
                    // $("#testcaselist").empty().hide();

                    // 선택된 항목이 지시하는 하위 카테고리 페이지의 URL얻기
                    var target = $(this).find("option:selected").attr("data-target");
                    var selector = "TestcaseList.html " + target;
                    console.log(selector)
                    // 다음 항목 로드
                    $("#testcaselist").load(selector, function() {
                        $(this).show();
                    });
                });

                $(document).on("change", "#testcaselist > select", function() {
                    var paramElement = $("<tr>")
                    var paramNameInput = $('<td>');
                    paramNameInput.attr({
                        "type": "text",
                        "class": "paramName",
                    });

                    paramNameInput.html("Adsfasdf");

                    var paramValueInput = $('<td>');
                    paramValueInput.attr({
                        "type": "text",
                        "class": "paramValue"
                    });
                    var paramDelButton = $('<td>');
                    paramDelButton.attr({
                        "type": "button",
                        "value": "Delete",
                        "class": "deleteParamFormat"
                    });
                    paramElement.append(paramNameInput).append(paramValueInput).append(paramDelButton);
                    var paramListRoot = $('#paramTable');
                    paramListRoot.append(paramElement);
                });

                // Deleting the saved parameter list
                $(document).on("click", '.deleteTestcase', function () {
                    $(this).closest('tr').remove();
                });
            });
